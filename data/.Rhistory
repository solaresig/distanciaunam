angle <- function(x,y){
dot.prod <- x%*%y
norm.x <- norm(x,type="2")
norm.y <- norm(y,type="2")
theta <- acos(dot.prod / (norm.x * norm.y))
as.numeric(theta)
}
llenararith<-function(x){
for(i in 1:length(x)){
k=1
if(!is.na(x[i])|(i==length(x))|(i==1)){
x[i]<-x[i]
}
else{
while((is.na(x[i+k]))&(i+k<length(x))){
k=k+1
}
x[i]<-x[i-1]+((x[i+k]-x[i-1])/(k+1))
}
}
return(x)
}
llenararith2<-function(x){
for(i in 1:length(x)){
k=1
if(!is.na(x[i])|(i==length(x))|(i==1)){
x[i]<-x[i]
}
else if(is.na(x[i-1])){
while((is.na(x[i+k]))&(i+k<length(x)+1)){
k=k+1
}
x[i]<-x[i+k]
}
else{
while((is.na(x[i+k]))&(i+k<length(x))){
k=k+1
}
if(!is.na(x[i+k])){
x[i]<-x[i-1]+((x[i+k]-x[i-1])/(k+1))
}
else{
x[i]<-x[i-1]
}
}
}
for(i in 1:length(x)){
if(i==1&is.na(x[i])){
x[i]<-x[i+1]
} else if(i==length(x)&is.na(x[i])){
x[i]<-x[i-1]
} else{
x[i]<-x[i]
}
}
return(x)
}
avgsil<-function(df, k){
km.res<-kmeans(df, centers=k, iter.max = 20, nstart = 25)
ss<-silhouette(km.res$cluster, dist(df))
return(mean(ss[,3]))
}
